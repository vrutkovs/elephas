<?xml version="1.0" encoding="UTF-8"?>
<interface domain="elephas">
  <!-- interface-requires gtk+ 3.10 -->
  <object class="GtkBox" id="viewswitcher">
    <property name="visible">True</property>
    <property name="vexpand">False</property>
    <property name="vexpand">False</property>
    <child>
      <object class="GtkButton" id="home-timeline-button">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="valign">center</property>
        <property name="sensitive">True</property>
        <property name="tooltip_text" translatable="yes">Home Timeline</property>
        <style>
              <class name="image-button"/>
        </style>
        <child>
          <object class="GtkImage" id="home-timeline-image">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="icon-name">user-home-symbolic</property>
            <property name="icon-size">1</property>
          </object>
        </child>
      </object>
      <packing>
        <property name="pack_type">start</property>
      </packing>
    </child>
    <child>
      <object class="GtkButton" id="notifications-button">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="valign">center</property>
        <property name="sensitive">True</property>
        <property name="tooltip_text" translatable="yes">Notifications</property>
        <style>
              <class name="image-button"/>
        </style>
        <child>
          <object class="GtkImage" id="notifications-image">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="icon-name">preferences-system-notifications-symbolic</property>
            <property name="icon-size">1</property>
          </object>
        </child>
      </object>
      <packing>
        <property name="pack_type">start</property>
      </packing>
    </child>
    <child>
      <object class="GtkButton" id="local-timeline-button">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="valign">center</property>
        <property name="sensitive">True</property>
        <property name="tooltip_text" translatable="yes">Local Timeline</property>
        <style>
              <class name="image-button"/>
        </style>
        <child>
          <object class="GtkImage" id="local-timeline-image">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="icon-name">system-users-symbolic</property>
            <property name="icon-size">1</property>
          </object>
        </child>
      </object>
      <packing>
        <property name="pack_type">start</property>
      </packing>
    </child>
    <child>
      <object class="GtkButton" id="federated-timeline-button">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="valign">center</property>
        <property name="sensitive">True</property>
        <property name="tooltip_text" translatable="yes">Federated Timeline</property>
        <style>
              <class name="image-button"/>
        </style>
        <child>
          <object class="GtkImage" id="federated-timeline-image">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="icon-name">network-workgroup-symbolic</property>
            <property name="icon-size">1</property>
          </object>
        </child>
      </object>
      <packing>
        <property name="pack_type">start</property>
      </packing>
    </child>
  </object>
</interface>
